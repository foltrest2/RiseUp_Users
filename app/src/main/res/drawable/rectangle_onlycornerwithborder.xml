<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#8000F5A0"
                android:endColor="#8000D9F5"
                android:type="linear"
                android:angle="0"/>
        </shape>
    </item>
    <item
        android:left="48dp"
        android:right="48dp">
        <shape android:shape="rectangle">

            <gradient
                android:startColor="#000000"
                android:endColor="#190232"
                android:type="linear"
                android:angle="-90"/>

        </shape>
    </item>
    <item
        android:top="48dp"
        android:bottom="48dp">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#000000"
                android:endColor="#190232"
                android:type="linear"
                android:angle="-90"/>
        </shape>
    </item>
    <item
        android:left="5dp"
        android:right="5dp"
        android:bottom="5dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#000000"
                android:endColor="#190232"
                android:type="linear"
                android:angle="-90"/>
        </shape>
    </item>
</layer-list>